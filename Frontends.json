{
    "twitter.com":{
        "frontend":"https://nitter.foss.wtf/",
        "rules":[
            "SetPath [fullpath]"
        ],
        "include":[
            "mobile"
        ]
    },
    "imgur.com":{
        "frontend":"https://img.foss.wtf/",
        "rules":[
            "([path.last]) >>> SetPath 'gallery/'",
            "([path.last]) >>> AppendPath [path.last]"
        ]
    },
    "youtube.com":{
        "frontend":"https://tube.foss.wtf/",
        "rules":[
            "SetPath [fullpath]"
        ],
        "include":[
            "m"
        ]
    },
    "youtu.be":{
        "frontend":"https://tube.foss.wtf/",
        "rules":[
            "([path]) >>> AddParam 'v',[path]",
            "SetPath 'watch'"
        ]
    },
    "quora.com":{
        "frontend":"https://quetre.foss.wtf/",
        "rules":[
            "SetPath [fullpath]"
        ]
    },
    "reddit.com":{
        "frontend":"https://teddit.net/",
        "rules":[
            "SetPath [fullpath]"
        ],
        "include":[
            "old",
            "i"
        ]
    },
    "medium.com":{
        "frontend":"https://libmedium.batsense.net/",
        "rules":[
            "SetPath [fullpath]"
        ]
    },
    "google.com":{
        "frontend":"https://serx.ml/",
        "rules":[
            "([params.tbm]|==|'isch') >>> AddParam 'categories','images'",
            "([params.tbm]|==|'nws') >>> AddParam 'categories','news'",
            "([params.tbm]|==|'vid') >>> AddParam 'categories','videos'",
            "([params.q]) >>> AddParam 'q',[params.q]",
            "SetPath 'search'"
        ]
    }
}